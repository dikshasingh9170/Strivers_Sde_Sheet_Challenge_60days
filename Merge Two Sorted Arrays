import java.io.*;
import java.util.* ;

public class Solution {
    public static int[] ninjaAndSortedArrays(int arr1[], int arr2[], int m, int n) {
        int j=0;
        for(int i=m;i<m+n;i++){
            arr1[i]=arr2[j];
            j++;
        }
        Arrays.sort(arr1);
        return arr1;
    }
}
